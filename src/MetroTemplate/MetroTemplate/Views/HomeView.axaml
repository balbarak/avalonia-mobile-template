<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="390" d:DesignHeight="844"
			 xmlns:ctl="using:MetroTemplate.Controls"
			 xmlns:views="using:MetroTemplate.Views"
			 Background="{DynamicResource PageBackground}"
			 x:Class="MetroTemplate.Views.HomeView">



	<Grid Classes="page">

		
		<Grid Classes="page-content">
			<StackPanel>
				<Button Command="{Binding GoToSecondViewCommand}">
					Go To Second View
				</Button>
			</StackPanel>
		</Grid>
	</Grid>
</UserControl>
